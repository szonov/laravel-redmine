{
    "name": "szonov/laravel-redmine",
    "description": "Laravel wrapper for the Redmine API",
    "keywords": [
        "laravel",
        "redmine",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Zonov",
            "email": "sergey@zonov.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "kbsali/redmine-api": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "SZonov\\Redmine\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SZonov\\Redmine\\RedmineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
